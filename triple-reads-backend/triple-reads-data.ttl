@prefix ex: <http://example.org/> .
@prefix schema: <http://schema.org/> .

# Admin
ex:admin1 ex:hasEmail "admin@example.com" .

# Genres
ex:Fantasy a ex:Genre ;
    schema:name "Fantasy" .

